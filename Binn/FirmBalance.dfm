  object Query_Firm: TADOQuery
    Connection = fmMain.Lis
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'type_contract'
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT'#9'contract_id AS id, firm_customer_name + '#39' ( '#39'  + contract' +
        '_cod + '#39' '#1086#1090' '#39' + convert(varchar(10),  date_begin, 104) + '#39')     ' +
        '# '#39' + firm_self_name + '#39' #'#39'AS name,'
      '        date_end, contract_cod, date_begin'
      'FROM'#9'view_contract_rights'
      'WHERE '#9'type_contract=:type_contract'
      #9'AND date_end >= GETDATE() - 31'
      'ORDER BY firm_customer_name, contract_id, contract_cod')
    Left = 202
    Top = 115
  end
