  object Query_Folder: TADOQuery
    CursorType = ctStatic
    LockType = ltReadOnly
    Parameters = <
      item
        Name = 'doc_type_cod'
        DataType = ftString
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'DECLARE @users_group_id int'
      
        'SELECT @users_group_id = users_group_id FROM users WHERE users_n' +
        'ame = SYSTEM_USER'
      ''
      'DECLARE @doc_type_cod varchar(200)'
      'SELECT @doc_type_cod = :doc_type_cod'
      ''
      'DECLARE @doc_type_id int'
      
        'SELECT @doc_type_id = inf_obj_id FROM inf_obj WHERE type_inf_id ' +
        '= -101 AND inf_obj_cod = @doc_type_cod'
      ''
      
        'SELECT doc_folder_id, parent_id = ISNULL(parent_id, -9), folder_' +
        'name'
      'FROM DOC_FOLDER'
      'WHERE users_group_id = @users_group_id'
      'AND doc_type_id = @doc_type_id'
      'UNION ALL'
      '-- '#1044#1086#1073#1072#1074#1080#1084' '#1088#1086#1076#1080#1090#1077#1083#1100#1089#1082#1091#1102' '#1087#1072#1087#1082#1091' ('#1090#1080#1087' '#1076#1086#1082#1091#1084#1077#1085#1090#1072')'
      'SELECT -9, null, inf_obj_name'
      'FROM INF_OBJ'
      'WHERE inf_obj_id = @doc_type_id'
      'ORDER BY folder_name')
    Left = 8
    Top = 81
  end
